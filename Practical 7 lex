%{
#include "y.tab.h"
#include <stdlib.h>
%}

%%
[0-9]+      { yylval = atoi(yytext); return NUMBER; }
"+"         { return PLUS; }
"-"         { return MINUS; }
"*"         { return MULT; }
"/"         { return DIV; }
[\t ]+      ;
\n          { return '\n'; }
.           { return yytext[0]; }
%%

int yywrap() { return 1; }
